        <arguments>
            <argument name="themes" xsi:type="array">
                <item name="adminhtml" xsi:type="string">Netbase/admintheme</item>
            </argument>
        </arguments>
    </type>
    <preference for="Magento\Backend\Block\Dashboard\Diagrams" type="Netbaseteam\Admintheme\Block\Dashboard\Diagrams" />
    <preference for="Magento\Backend\Block\Dashboard\Tab\Amounts" type="Netbaseteam\Admintheme\Block\Dashboard\Tab\Amounts" />
    <preference for="Magento\Backend\Block\Dashboard\Tab\Orders" type="Netbaseteam\Admintheme\Block\Dashboard\Tab\Orders" />
</config>