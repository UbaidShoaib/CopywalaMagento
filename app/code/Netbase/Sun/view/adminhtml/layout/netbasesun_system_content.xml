<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd"> 
	<head>
        <css src="Netbase_Sun::css/bootstrap.css" /> 
        <css src="Netbase_Sun::css/sun.css" /> 
    </head>
    <body>
    	<referenceBlock name="page.title">
            <action method="setPageTitle">
		        <argument translate="true" name="title" xsi:type="string">Netbase Content Config</argument>
		    </action> 
        </referenceBlock>  
        <referenceContainer name="content">
            <block class="Netbase\Sun\Block\Adminhtml\Content" name="sun_config_content" template="Netbase_Sun::system/config/content.phtml"> 
            </block>
        </referenceContainer>
    </body>
</page>